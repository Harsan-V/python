arr = [2, 5, 7, 11, 14, 18, 20]

target = int(input('Enter a number : '))

arr.sort()

left = 0
right = len(arr)-1
found = False
while left <=right:
    mid = (left+right)//2
    if arr[mid]==target:
        print('Element Found at Index : ',mid)
        found = True
        break
    elif arr[mid] < target:
        left = mid+1
    else:
        right = mid-1
if not found:
    print('Element Not Found !')
