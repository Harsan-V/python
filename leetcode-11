#optimal Solution
class Solution(object):
    def maxArea(self, height):
        left = 0
        right = len(height)-1
        max_val = 0
        while left<right:
            width = right-left
            area = min(height[right],height[left])*width
            max_val = max(max_val,area)
            if height[left]<=height[right]:
                left +=1
            else:
                right -=1
        return max_val
        
