# best optimized way

class Solution(object):
    def fizzBuzz(self, n):
        res = [""] * n   # pre-allocate list (slightly better than append)

        for i in range(1, n + 1):
            s = ""
            if i % 3 == 0:
                s += "Fizz"
            if i % 5 == 0:
                s += "Buzz"
            res[i - 1] = s if s else str(i)

        return res
